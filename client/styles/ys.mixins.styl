// Auto prefix mixins.
height_calc(args...)
	height calc(args)
	height -webkit-calc(args)
	height -moz-calc(args)

width_calc(args...)
	width calc(args)
	width -webkit-calc(args)
	width -moz-calc(args)

// Clearfix from bootstrap.
clearfix()
	&:before, &:after
		content: " ";
		display: table;
	&:after
		clear: both;
